dy>\n          <tr v-for=\"user in users\" :key=\"user.id\">\n            <td>{{ user.name }}</td>\n            <td>{{ user.email }}</td>\n            <td>{{ user.country }}</td>\n            <td>\n              <va-badge :color=\"getStatusColor(user.status)\">\n                {{ user.status }}\n              </va-badge>\n            </td>\n          </tr>\n        </tbody>\n      </table>\n    </va-card>\n\n    <va-card :title=\"$t('tables.stripedHoverable')\">\n      <table class=\"va-table va-table--striped va-table--hoverable\">\n        <thead>\n          <tr>\n            <th>Name</th>\n            <th>Email</th>\n            <th>Country</th>\n            <th>Status</th>\n          </tr>\n        </thead>\n\n        <tbody>\n          <tr v-for=\"user in users\" :key=\"user.id\">\n            <td>{{ user.name }}</td>\n            <td>{{ user.email }}</td>\n            <td>{{ user.country }}</td>\n            <td>\n              <va-badge :color=\"getStatusColor(user.status)\">\n                {{ user.status }}\n              </va-badge>\n            </td>\n          </tr>\n        </tbody>\n      </table>\n    </va-card>\n  </div>\n</template>\n\n<script>\nimport data from './data.json'\n\nexport default {\n  data () {\n    return {\n      users: data.slice(0, 8),\n    }\n  },\n  methods: {\n    getStatusColor (status) {\n      if (status === 'paid') {\n        return 'success'\n      }\n\n      if (status === 'processing') {\n        return 'info'\n      }\n\n      return 'danger'\n    },\n  },\n}\n</script>\n\n<style lang=\"scss\">\n  .markup-tables {\n    .va-table {\n      width: 100%;\n    }\n\n    .va-card {\n      max-width: 1200px;\n    }\n  }\n</style>\n"]}]}