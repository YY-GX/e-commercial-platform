AACb,CAAP,IAAY,CAAZ;AACD,GALM,MAKA,IAAIgB,OAAJ,EAAa;AAClBJ,IAAAA,KAAK,CAACZ,CAAN,IAAW,EAAX;AACAY,IAAAA,KAAK,CAACX,CAAN,IAAW,CAAX;AACAW,IAAAA,KAAK,CAACV,CAAN,IAAW,CAAX;AAEAW,IAAAA,MAAM,CAACb,CAAP,IAAY,CAAZ;AACAa,IAAAA,MAAM,CAACZ,CAAP,IAAY,CAAZ;AACAY,IAAAA,MAAM,CAACX,CAAP,IAAY,CAAZ;AACD,GARM,MAQA,IAAIe,MAAJ,EAAY;AACjBL,IAAAA,KAAK,CAACZ,CAAN,IAAW,EAAX;AACAY,IAAAA,KAAK,CAACX,CAAN,IAAW,CAAX;AACAW,IAAAA,KAAK,CAACV,CAAN,IAAW,CAAX;AACD;;AAED,MAAIgB,gBAAJ,EAAsB;AACpBN,IAAAA,KAAK,CAACV,CAAN,IAAW,CAAX;AACAW,IAAAA,MAAM,CAACX,CAAP,IAAY,CAAZ;AACD,GArCwC,CAuCzC;;;AACA,MAAIU,KAAK,CAACZ,CAAN,GAAU,CAAd,EAAiB;AAAEY,IAAAA,KAAK,CAACZ,CAAN,IAAW,GAAX;AAAgB;;AACnC,MAAIY,KAAK,CAACZ,CAAN,GAAU,CAAd,EAAiB;AAAEL,IAAAA,IAAI,CAACQ,KAAL,CAAWS,KAAK,CAACZ,CAAN,GAAUY,KAAK,CAACZ,CAAN,GAAU,GAA/B;AAAqC;;AACxD,MAAIa,MAAM,CAACb,CAAP,GAAW,CAAf,EAAkB;AAAEa,IAAAA,MAAM,CAACb,CAAP,IAAY,GAAZ;AAAiB;;AACrC,MAAIa,MAAM,CAACb,CAAP,GAAW,CAAf,EAAkB;AAAEL,IAAAA,IAAI,CAACQ,KAAL,CAAWU,MAAM,CAACb,CAAP,GAAWa,MAAM,CAACb,CAAP,GAAW,GAAjC;AAAuC;;AAE3DY,EAAAA,KAAK,CAACX,CAAN,GAAUW,KAAK,CAACX,CAAN,GAAU,CAAV,GAAcW,KAAK,CAACX,CAApB,GAAwB,CAAlC;AACAW,EAAAA,KAAK,CAACX,CAAN,GAAUW,KAAK,CAACX,CAAN,GAAU,GAAV,GAAgBW,KAAK,CAACX,CAAtB,GAA0B,GAApC;AACAY,EAAAA,MAAM,CAACZ,CAAP,GAAWY,MAAM,CAACZ,CAAP,GAAW,CAAX,GAAeY,MAAM,CAACZ,CAAtB,GAA0B,CAArC;AACAY,EAAAA,MAAM,CAACZ,CAAP,GAAWY,MAAM,CAACZ,CAAP,GAAW,GAAX,GAAiBY,MAAM,CAACZ,CAAxB,GAA4B,GAAvC;AAEAW,EAAAA,KAAK,CAACV,CAAN,GAAUU,KAAK,CAACV,CAAN,GAAU,CAAV,GAAcU,KAAK,CAACV,CAApB,GAAwB,CAAlC;AACAU,EAAAA,KAAK,CAACV,CAAN,GAAUU,KAAK,CAACV,CAAN,GAAU,GAAV,GAAgBU,KAAK,CAACV,CAAtB,GAA0B,GAApC;AACAW,EAAAA,MAAM,CAACX,CAAP,GAAWW,MAAM,CAACX,CAAP,GAAW,CAAX,GAAeW,MAAM,CAACX,CAAtB,GAA0B,CAArC;AACAW,EAAAA,MAAM,CAACX,CAAP,GAAWW,MAAM,CAACX,CAAP,GAAW,GAAX,GAAiBW,MAAM,CAACX,CAAxB,GAA4B,GAAvC;AAEA,SAAO,CAACU,KAAK,CAACrB,GAAP,EAAYsB,MAAM,CAACtB,GAAnB,CAAP;AACD,CAxDM;AA0DP,OAAO,IAAM4B,qBAAqB,GAAG,SAAxBA,qBAAwB,CAACX,KAAD,EAAW;AAAA,0BACxBG,gBAAgB,CAACH,KAAD,CADQ;AAAA;AAAA,MACvCY,IADuC;AAAA,MACjCC,KADiC;;AAE9C,6CAAoCD,IAApC,eAA6CC,KAA7C;AACD,CAHM","sourcesContent":["export const hex2rgb = (hex, opacity) => {\r\n  hex = (hex + '').trim()\r\n\r\n  let rgb = null\r\n  let match = hex.match(/^#?(([0-9a-zA-Z]{3}){1,3})$/)\r\n\r\n  if (!match) {\r\n    return null\r\n  }\r\n\r\n  rgb = {}\r\n\r\n  hex = match[1]\r\n\r\n  if (hex.length === 6) {\r\n    rgb.r = parseInt(hex.substring(0, 2), 16)\r\n    rgb.g = parseInt(hex.substring(2, 4), 16)\r\n    rgb.b = parseInt(hex.substring(4, 6), 16)\r\n  } else if (hex.length === 3) {\r\n    rgb.r = parseInt(hex.substring(0, 1) + hex.substring(0, 1), 16)\r\n    rgb.g = parseInt(hex.substring(1, 2) + hex.substring(1, 2), 16)\r\n    rgb.b = parseInt(hex.substring(2, 3) + hex.substring(2, 3), 16)\r\n  }\r\n\r\n  rgb.css = 'rgb' + (opacity ? 'a' : '') + '('\r\n  rgb.css += rgb.r + ',' + rgb.g + ',' + rgb.b\r\n  rgb.css += (opacity ? ',' + opacity : '') + ')'\r\n\r\n  return rgb\r\n}\r\n\r\nexport const hex2hsl = (H) => {\r\n  /*\r\n   * the source text is taken from here (with minor modifications):\r\n   * https://css-tricks.com/converting-color-spaces-in-javascript/\r\n   */\r\n\r\n  // Convert hex to RGB first. Ignore opacity\r\n  let { r, g, b } = hex2rgb(H, 1)\r\n\r\n  // Then to HSL\r\n  r /= 255\r\n  g /= 255\r\n  b /= 255\r\n\r\n  let cmin = Math.min(r, g, b)\r\n  let cmax = Math.max(r, g, b)\r\n  let delta = cmax - cmin\r\n  let h = 0\r\n  let s = 0\r\n  let l = 0\r\n\r\n  if (delta === 0) { h = 0 } else if (cmax === r) { h = ((g - b) / delta) % 6 } else if (cmax === g) { h = (b - r) / delta + 2 } else { h = (r - g) / delta + 4 }\r\n\r\n  h = Math.round(h * 60)\r\n\r\n  if (h < 0) { h += 360 }\r\n\r\n  l = (cmax + cmin) / 2\r\n  s = delta === 0 ? 0 : delta / (1 - Math.abs(2 * l - 1))\r\n  s = +(s * 100).toFixed(1)\r\n  l = +(l * 100).toFixed(1)\r\n\r\n  const HSL = {\r\n    h: Math.round(h),\r\n    s: Math.round(s),\r\n    l: Math.round(l),\r\n\r\n    get css () {\r\n      return 'hsl(' + HSL.h + ',' + HSL.s + '%,' + HSL.l + '%)'\r\n    },\r\n  }\r\n\r\n  return HSL\r\n}\r\n\r\nexport const getBoxShadowColor = (color) => {\r\n  return hex2rgb(color, 0.6).css\r\n}\r\n\r\nexport const getHoverColor = (color) => {\r\n  return hex2rgb(color, 0.2).css\r\n}\r\n\r\nexport const getFocusColor = (color) => {\r\n  return hex2rgb(color, 0.3).css\r\n}\r\n\r\nexport const getGradientColor = (color) => {\r\n  let first = hex2hsl(color)\r\n  let second = hex2hsl(color)\r\n\r\n  // hue circle degrees approximation\r\n  const isRed = (first.h >= 0 && first.h < 45) || (first.h >= 285)\r\n  const isYellow = first.h >= 45 && first.h < 85\r\n  const isGreen = first.h >= 85 && first.h < 165\r\n  const isBlue = first.h >= 165 && first.h < 285\r\n  const isUndersaturated = first.s < 30 // i.e. too pale, gray-ish, monotone\r\n\r\n  if (isRed) {\r\n    first.h += 11\r\n    first.s += 27\r\n    first.l += 8\r\n  } else if (isYellow) {\r\n    first.h += 3\r\n    first.l += 9\r\n\r\n    second.h -= 2\r\n  } else if (isGreen) {\r\n    first.h += 13\r\n    first.s -= 5\r\n    first.l += 7\r\n\r\n    second.h -= 3\r\n    second.s -= 1\r\n    second.l -= 6\r\n  } else if (isBlue) {\r\n    first.h -= 15\r\n    first.s += 3\r\n    first.l += 2\r\n  }\r\n\r\n  if (isUndersaturated) {\r\n    first.l += 6\r\n    second.l -= 2\r\n  }\r\n\r\n  // restrictions and validations\r\n  if (first.h < 0) { first.h += 360 }\r\n  if (first.h > 0) { Math.round(first.h = first.h % 360) }\r\n  if (second.h < 0) { second.h += 360 }\r\n  if (second.h > 0) { Math.round(second.h = second.h % 360) }\r\n\r\n  first.s = first.s > 0 ? first.s : 0\r\n  first.s = first.s < 100 ? first.s : 100\r\n  second.s = second.s > 0 ? second.s : 0\r\n  second.s = second.s < 100 ? second.s : 100\r\n\r\n  first.l = first.l > 0 ? first.l : 0\r\n  first.l = first.l < 100 ? first.l : 100\r\n  second.l = second.l > 0 ? second.l : 0\r\n  second.l = second.l < 100 ? second.l : 100\r\n\r\n  return [first.css, second.css]\r\n}\r\n\r\nexport const getGradientBackground = (color) => {\r\n  const [left, right] = getGradientColor(color)\r\n  return `linear-gradient(to right, ${left}, ${right})`\r\n}\r\n"]}]}